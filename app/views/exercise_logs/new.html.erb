<h1>New Log</h1>
<% @exercises.each do |exercise| %>
<h3><%= exercise.exercise_option.name %></h3>
  <%= simple_form_for([@exercise, @exercise_log], url: program_exercise_logs_path(@program, @exercise)) do |f| %>
    <%= f.input :sets %>
    <%= f.input :reps %>
    <%= f.input :weight %>
    <hr>
    <%= f.button :submit %>
    <% end %>
<% end %>
