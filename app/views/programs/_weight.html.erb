<script>
console.log("weight!!")
  var id = <%= raw id.to_json %>

  function renderChart(exercise) {
    var ctx = document.getElementById("myChartexercise_" + id);
    var xAxis = JSON.parse($('#dateexercise_' + id).attr("value"));
    var yAxis = JSON.parse($('#weightexercise_' + id).attr("value"));
    var myChart = new Chart(ctx, {
      type: 'line',
      data: {
          labels: xAxis,
          datasets: [{
              label: 'Weight',
              fill: true,
              data: yAxis,
              backgroundColor: [
                  'rgba(128, 191, 216, 0.2)'
              ],
              borderColor: [
                  'rgba(40, 44, 60, 1)'
              ],
              borderWidth: 1
          }]
      },
      options: {
          scales: {
              yAxes: [{
                  ticks: {
                      beginAtZero:true
                  }
              }]
          }
      }
    });
  };


  renderChart(id)

</script>
